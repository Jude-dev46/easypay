{
  "expo": {
    "name": "easyPay",
    "slug": "easyPay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#312e81"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "EasyPay needs access to your photos to let you upload it."
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow EasyPay to use Face ID."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#312e81"
      },
      "package": "com.jude_oscar12.easyPay"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ce1497eb-a7cf-4699-a72b-467af73dcd37"
      }
    }
  }
}
